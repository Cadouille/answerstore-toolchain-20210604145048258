{
  "version": 1,
  "cloudant_url": {
    "searchPatterns": [
      "cloudfoundry:$['cloudantNoSQLDB'][0].credentials.url",
      "env:service_cloudant:$.url",
      "env:cloudant_url",
      "env:CLOUDANT_URL",
      "file:/server/localdev-config.json:$.url"
    ]
  },
  "cloudant_apikey": {
    "searchPatterns": [
      "cloudfoundry:$['cloudantNoSQLDB'][0].credentials.apikey",
      "env:service_cloudant:$.apikey",
      "env:cloudant_apikey",
      "env:CLOUDANT_APIKEY",
      "file:/server/localdev-config.json:$.apikey"
    ]
  },
  "database_name": {
    "searchPatterns": ["env:database_name", "env:DATABASE_NAME"]
  }
}
